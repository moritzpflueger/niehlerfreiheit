{
  "version": 2,
  "functions": {
    "index.php": {
      "runtime": "vercel-php@0.7.1"
    }
  },
  "routes": [
    {
      "src": "/(assets|media|content)/.*",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.php"
    }
  ],
  "outputDirectory": ".",
  "buildCommand": "npm run build"
}

